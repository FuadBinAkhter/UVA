#include<bits/stdc++.h>
using namespace std;
#define fastio ios_base::sync_with_stdio(false);cin.tie(NULL);
#define PI 2*acos(0.0)
int main()
{
    fastio;
    int t,i=1;
    double r;
    cin>>t;
    while(t>0)
    {
        cin>>r;
        printf("Case %d: %.2lf\n",i++,(4*r*r)-(PI*r*r));
        t--;
    }
    return 0;
}
